<SCRIPT LANGUAGE="JavaScript">

<!-- Begin hiding JavaScript language from (older) browsers.

function ScrollBottomScreen(Start)
{
    var m1  = " ..... Welcome to the What's New Home Page!";
    var m2  = " ..... While here, read the OPENline, published by our Public Information Office.";
    var msg=m1+m2;
    var out = " ";
    var c   = 1;

    if (Start > 100)
    {
        Start--;
        var cmd="ScrollBottomScreen(" + Start + ")";
        timerTwo=window.setTimeout(cmd,100);
    }
    else if (Start <= 100 && Start > 0)
    {
        for (c=0 ; c < Start ; c++) {
            out+=" ";
        }

        out+=msg;
        Start--;
        var cmd="ScrollBottomScreen(" + Start + ")";
        window.status=out;
        timerTwo=window.setTimeout(cmd,100);
    }
    else if (Start <= 0)
    {
        if (-Start < msg.length)
        {
            out+=msg.substring(-Start,msg.length);
            Start--;
            var cmd="ScrollBottomScreen(" + Start + ")";
            window.status=out;
            timerTwo=window.setTimeout(cmd,100);
        }
        else
        {
            window.status=" ";
            timerTwo=window.setTimeout("ScrollBottomScreen(100)",50);
        }
    }
}
// -- End of JavaScript code -------------- -->
</SCRIPT>
</HEAD>


