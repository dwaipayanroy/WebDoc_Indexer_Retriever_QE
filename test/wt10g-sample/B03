<html>
<HEAD>
<TITLE>
Relocating to Mesa</TITLE>


<SCRIPT LANGUAGE="JavaScript">

<!-- Begin hiding JavaScript language from (older) browsers.

function ScrollBottomScreen(Start)
{
    var m1  = " ..... Welcome to the Relocating to Mesa Home Page!";
    var m2  = " ..... Are you planning on moving to Mesa?";
    var m3  = " ..... Take a look at the City Services Home Page!!!!";
    var msg=m1+m2+m3;
    var out = " ";
    var c   = 1;

    if (Start > 100)
    {
        Start--;
        var cmd="ScrollBottomScreen(" + Start + ")";
        timerTwo=window.setTimeout(cmd,100);
    }
    else if (Start <= 100 && Start > 0)
    {
        for (c=0 ; c < Start ; c++) {
            out+=" ";
        }

        out+=msg;
        Start--;
        var cmd="ScrollBottomScreen(" + Start + ")";
        window.status=out;
        timerTwo=window.setTimeout(cmd,100);
    }
    else if (Start <= 0)
    {
        if (-Start < msg.length)
        {
            out+=msg.substring(-Start,msg.length);
            Start--;
            var cmd="ScrollBottomScreen(" + Start + ")";
            window.status=out;
            timerTwo=window.setTimeout(cmd,100);
        }
        else
        {
            window.status=" ";
            timerTwo=window.setTimeout("ScrollBottomScreen(100)",50);
        }
    }
}
// -- End of JavaScript code -------------- -->
</SCRIPT>
</HEAD>


